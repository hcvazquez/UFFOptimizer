{
  "name": "chomsky",
  "version": "1.0.8",
  "description": "A simple ES6 i18n engine.",
  "scripts": {
    "start": "npm run serve",
    "build:demo": "gulp",
    "watch:demo": "gulp watch",
    "clean:demo": "gulp clean",
    "test": "npm run unit",
    "unit": "karma start --single-run",
    "tdd": "karma start",
    "serve": "concurrently \"npm run build:demo\" \"http-server public\"",
    "precompile": "./node_modules/.bin/del lib",
    "compile": "babel src --ignore .spec.js --out-dir lib",
    "compile:watch": "babel src --ignore .spec.js --out-dir lib --watch",
    "prepublish": "npm run compile",
    "lint": "eslint lib"
  },
  "main": "lib/chomsky.js",
  "author": "Kameron R. Sween <kameron@krsween.com>",
  "repository": "https://github.com/bullhorn/chomsky",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "concurrently": "^2.0.0",
    "del": "2.2.0",
    "del-cli": "0.2.0",
    "eslint": "^2.9.0",
    "eslint-config-bullhorn": "^0.1.0",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "http-server": "^0.9.0",
    "intl": "^1.1.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.5",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-source-map-support": "^1.1.0",
    "karma-spec-reporter": "0.0.26",
    "phantomjs-prebuilt": "^2.1.7",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
	"lazypipe": "^1.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "moment": "^2.13.0",
    "moment-timezone": "^0.5.4",
    "numbro": "^1.7.1",
    "rxjs": "^5.0.0-beta.6",
    "systemjs": "^0.19.27"
  }
}
